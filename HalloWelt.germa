öffentlich Klasse HalloWelt ist Vergleichbar<HalloWelt> {

	Zeichenkette text = null;

	öffentlich stationär nichts Einstieg(Zeichenkette[] argus) {
		System.raus.druckeZeile("Hallo Welt!");
	}

	öffentlich HalloWelt(Zeichenkette text){
		ich.text = text;
	}

	öffentlich nichts SageText() {
		wenn (ich.text != null) {
			System.raus.druckeZeile(ich.text);
		}
	}

	öffentlich Zahl vergleicheMit( HalloWelt Mars ){
		zurück ich.text.vergleicheMit(Mars.text);
	}
}
